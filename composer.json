{
	"name": "damejidlo/phpstan-nette-tester",
	"type": "phpstan-extension",
	"description": "PHPStan nette/tester extension",
	"keywords": ["phpstan", "nette", "tester"],
	"license": "MIT",
	"require": {
		"php": ">=7.2",
		"nikic/php-parser": "^4.0",
		"phpstan/phpstan": "^0.12.0"
	},
	"require-dev": {
		"damejidlo/coding-standard": "^2.0.0",
		"jakub-onderka/php-parallel-lint": "^1.0",
		"nette/tester": "^2.3.1",
		"phpunit/phpunit": "^7.5.18",
		"phpstan/phpstan-phpunit": "^0.12.5",
		"phpstan/phpstan-strict-rules": "^0.12.1"
	},
	"autoload": {
		"psr-4": {
			"Damejidlo\\PHPStan\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"DamejidloTests\\": "tests/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "0.2-dev"
		},
		"phpstan": {
			"includes": [
				"extension.neon"
			]
		}
	}
}
